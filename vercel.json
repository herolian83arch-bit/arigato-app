{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/locales/(.*)", "dest": "/locales/$1" },
    { "src": "/(.*\\.(js|css|json|png|jpg|jpeg|gif|svg|ico))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 